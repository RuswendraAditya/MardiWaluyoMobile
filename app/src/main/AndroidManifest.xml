<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bethesda.com.bethesdahospitalmobile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_bethesda"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BethesdaTheme">
        <activity android:name=".main.main.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.login.LoginActivity" />
        <activity android:name=".main.main.MainMenuActivity" />
        <activity android:name=".main.registration.KlinikPickerActivity" />
        <activity android:name=".main.registration.RegistrationActivity" />
        <activity android:name=".main.registration.DokterPickerActivity"></activity>
    </application>

</manifest>